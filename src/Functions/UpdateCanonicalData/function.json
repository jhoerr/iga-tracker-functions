{
  "disabled": false,
  "scriptFile": "../bin/Functions.dll",
  "entryPoint": "Ptp.UpdateCanonicalData.Function.Run",
  "bindings": [
    {
      "name": "command",
      "queueName": "%CommandQueue.Name%",
      "connection": "CommandQueue.Listen.ConnectionString",
      "accessRights": "listen",
      "type": "serviceBusTrigger",
      "direction": "in"
    },
    {
      "name": "nextCommand",
      "queueName": "%CommandQueue.Name%",
      "connection": "CommandQueue.Send.ConnectionString",
      "accessRights": "manage",
      "type": "serviceBus",
      "direction": "out"
    }
  ]
}
